{
  "name": "bluesky-youtube-privacy-bot",
  "version": "1.0.0",
  "description": "A Bluesky bot that monitors hashtags and converts YouTube URLs to privacy-friendly alternatives",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:netlify": "netlify dev",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "node tests/test-url.js && node tests/test-shorts.js && node tests/test-privacy-url.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "bluesky",
    "bot",
    "at-protocol",
    "youtube",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@atproto/api": "^0.12.0",
    "@atproto/lexicon": "^0.4.0",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@netlify/functions": "^2.0.0",
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
